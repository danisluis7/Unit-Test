package JavaAbstractFactoryPattern;

public interface Widget {
	boolean validate(String temp);
}

class Mail implements Widget{

	@Override
	public boolean validate(String temp) {
		// TODO Auto-generated method stub
		return temp.equals("lorence@enclave.vn");
	}	
}

class User implements Widget{

	@Override
	public boolean validate(String temp) {
		// TODO Auto-generated method stub
		return false;
	}
	
}

class Pass implements Widget{

	@Override
	public boolean validate(String temp) {
		// TODO Auto-generated method stub
		return false;
	}
	
}
